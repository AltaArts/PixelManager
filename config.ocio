colorspaces:

#Linear stuff:

  - !<ColorSpace>
    name: Linear Rec709/sRGB
    aliases: [Linear, Linear Tristimulus, Linear Rec709, Linear Rec.709 (sRGB)]
    family: Utility
    categories: [Linear]
    encoding: scene-linear
    equalitygroup: ""
    bitdepth: 32f
    description: |
      Linear Rec. 709/sRGB (Full Range), Native linear space
    isdata: false
    allocation: lg2
    allocationvars: [-12.473931188, 12.526068812]
    
  - !<ColorSpace>
    name: Linear Rec2020
    aliases: [Linear Rec.2020, Linear BT2020, Linear BT.2020]
    family: Utility
    categories: [Linear]
    encoding: scene-linear
    equalitygroup: ""
    bitdepth: 32f
    description: Wide-gamut linear space for Rec. 2020, used in UHD and HDR content creation.
    isdata: false
    allocation: lg2
    allocationvars: [-12.473931188, 12.526068812]
    from_reference: !<GroupTransform>
      children:
        - !<MatrixTransform> {matrix: [1.9070375449900, -0.6928777625154, -0.2143164997998, 0, -0.1622967285301, 1.3765783240526, -0.2141824465864, 0, -0.1275531208933, -0.2352480235664, 1.3628755821497, 0, 0, 0, 0, 1]}

  - !<ColorSpace>
    name: Linear ACES - AP0
    aliases: [ACES2065-1]
    family: Utility
    categories: [Linear]
    encoding: scene-linear
    equalitygroup: ""
    bitdepth: 32f
    description: ACES AP0 linear space, the native color space of the ACES pipeline.
      ACES AP0 linear space AKA ACES2065-1
    isdata: false
    allocation: lg2
    allocationvars: [-8, 5, 0.00390625]
    from_reference: !<GroupTransform>
      children:
        - !<FileTransform> {src: srgb_to_xyz.spimtx, interpolation: linear}
        - !<BuiltinTransform> {style: "UTILITY - ACES-AP0_to_CIE-XYZ-D65_BFD", direction: inverse}
		
  - !<ColorSpace>
    name: Linear ACES - AP1
    aliases: [ACEScg]
    family: Utility
    categories: [Linear]
    encoding: scene-linear
    equalitygroup: ""
    bitdepth: 32f
    description: ACES AP1 linear space, optimized for CGI and VFX in the ACES pipeline.
      ACES AP1 linear space AKA ACEScg
    isdata: false
    allocation: lg2
    allocationvars: [-8, 5, 0.00390625]
    from_reference: !<GroupTransform>
      children:
        - !<FileTransform> {src: srgb_to_xyz.spimtx, interpolation: linear}
        - !<BuiltinTransform> {style: "UTILITY - ACES-AP1_to_CIE-XYZ-D65_BFD", direction: inverse}
		
  - !<ColorSpace>
    name: Linear - E-Gamut
    family: Utility
    categories: [Linear]
    encoding: scene-linear
    equalitygroup: ""
    bitdepth: 32f
    description: Wide-gamut linear space used for grading and color correction.
    isdata: false
    allocation: lg2
    allocationvars: [-10, 7, 0.0056065625]
    to_reference: !<GroupTransform>
      children:
        - !<MatrixTransform> {matrix: [1.9070375449900, -0.6928777625154, -0.2143164997998, 0, -0.1622967285301, 1.3765783240526, -0.2141824465864, 0, -0.1275531208933, -0.2352480235664, 1.3628755821497, 0, 0, 0, 0, 1]}

#Display Conversions:

  - !<ColorSpace>
    name: sRGB
    aliases: [Gamma 2.2 Rec.709 - Texture, Input - Generic - sRGB - Texture, Gamma 2.2 Piecewise sRGB]
    family: Utility
    bitdepth: 32f
    categories: [Display_Encodings]
    encoding: data-spaces
    equalitygroup: ""
    description: sRGB colorspace with piecewise gamma 2.2 curve for display output.
    isdata: false
    allocation: uniform
    allocationvars: [-0.125, 4.875]
    to_reference: !<FileTransform> {src: srgb.spi1d, interpolation: linear}
    from_reference: !<FileTransform> {src: srgb_inv.spi1d, interpolation: linear}

  - !<ColorSpace>
    name: Filmic sRGB
    family: display
    categories: [Display_Encodings]
    encoding: sdr-spaces
    equalitygroup: ""
    bitdepth: 32f
    description: sRGB colorspace with a filmic curve for a more cinematic look.
      Filmic sRGB view transform
    isdata: false
    allocation: lg2
    allocationvars: [-12.473931188, 12.526068812]
    from_reference: !<GroupTransform>
        children:
            - !<ColorSpaceTransform> {src: Linear Rec709/sRGB, dst: Filmic Log}
            - !<FileTransform> {src: filmic_to_0-70_1-03.spi1d, interpolation: linear}

